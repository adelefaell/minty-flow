{
  "expo": {
    "version": "1.0.0",
    "name": "Minty Flow",
    "slug": "mint-flow",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7aafeeea-8f93-4fa3-b074-febd59362696"
      }
    },
    "owner": "adelefaells-organization",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "mintyflownative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mintyflownative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#CCFFD5",
        "foregroundImage": "./src/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/assets/images/android-icon-background.png",
        "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mintyflownative"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#CCFFD5",
          "dark": {
            "backgroundColor": "#007B17"
          }
        }
      ],
      "expo-localization",
      "./plugins/withWatermelonDBJSI.js",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-font",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your device motion"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
